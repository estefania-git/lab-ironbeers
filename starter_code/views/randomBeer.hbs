{{#each beers}}
<div class="random">
    <div class="random-img"><img src={{this.image_url}} alt=""></div>
    <h1>{{this.name}}</h1>
    <h2>{{this.tagline}}</h2>
    <p>{{this.description}}</p>
    {{#each this.food_pairing}}
    <li>{{this}}</li>
    {{/each}}
    <p>{{this.brewers_tips}}</p>
</div>
{{/each}}